Proceso EjercicioOperadoresAritmeticos
	
	Escribir 'Proporciona el primer valor: '
	Leer valor1;
	Escribir 'Proporciona el segundo valor: '
	Leer valor2;
	Imprimir 'Suma: ', valor1 +valor2;
	Imprimir 'Resta: ', valor1 - valor2;
	Imprimir 'Multiplicacion: ', valor1*valor2;
	Imprimir 'Division: ', valor1/valor2;
	Imprimir 'Modulo(residuo division): ', valor1 MOD valor2;
	Imprimir 'Potencia: ', valor1^valor2;
	
	
FinProceso
